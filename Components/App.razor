@inject IWebHostEnvironment Env
<!DOCTYPE html>
<html lang="en">



<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <link rel="stylesheet" href="@Assets["app.css"]" />
    <link rel="stylesheet" href="@Assets["dotnet.styles.css"]" />
    <ImportMap />
    <link rel="icon" type="image/png" href="favicon.png" />

    @if (Env.IsDevelopment()){
        
        <title>OW3N DEV</title>

        <meta name="apple-mobile-web-app-title" content="OW3N DEV">
    }
    else{
        <title>OW3N</title>
        <meta name="apple-mobile-web-app-title" content="OW3N">
    }
    

    <meta name="theme-color" content="#0F1417">
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link rel="apple-touch-icon" href="favicon.png" />

    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    
    
    <HeadOutlet />
</head>

<body>
    <Routes />
    <script src="_framework/blazor.web.js"></script>
    <script type="module" src="app.js"> </script>
</body>

</html>

